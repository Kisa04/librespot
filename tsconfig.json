{
  "include": [
    "src/**/*"
  ],
  "assets":["src/proto/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "outDir": "build"
  }
}
